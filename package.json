{
  "name": "api-assets",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "engines": {
    "node": ">=10.16.0",
    "npm": ">=6.9.0"
  },
  "scripts": {
    "build": "tsc",
    "debug": "tsnd --inspect --respawn src/server.ts",
    "start": "tsn ./src/server.ts",
    "dev": "NODE_ENV=development tsnd ./src/server.ts",
    "prod": "NODE_ENV=production node ./dist/server.js",
    "migrate:up": "db-migrate up --config ./src/helpers/configs/database.json -m ./src/helpers/migrations",
    "migrate:down": "db-migrate down -c 999 --config ./src/helpers/configs/database.json -m ./src/helpers/migrations",
    "db:prepare": "yarn migrate:down && yarn migrate:up"
  },
  "dependencies": {
    "aws-sdk": "~2.488.0",
    "axios": "~0.19.0",
    "body-parser": "~1.19.0",
    "class-transformer": "~0.2.3",
    "class-validator": "~0.9.1",
    "compression": "~1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "~2.8.5",
    "dotenv": "~8.0.0",
    "express": "~4.17.0",
    "form-data": "~2.5.0",
    "imagemin": "~7.0.0",
    "jsonwebtoken": "~8.5.1",
    "memory-cache": "~0.2.0",
    "multer": "~1.4.1",
    "multer-s3": "~2.9.0",
    "multer-sharp-s3": "~0.1.0",
    "mysql2": "~1.6.5",
    "nodemailer": "~6.3.0",
    "sequelize": "~5.8.10",
    "sequelize-typescript": "~1.0.0-beta.3",
    "sharp": "~0.20.8",
    "uuid": "~3.3.2"
  },
  "devDependencies": {
    "@aws/dynamodb-data-mapper": "~0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "~0.7.3",
    "@types/axios": "~0.14.0",
    "@types/bluebird": "~3.5.27",
    "@types/cookie-parser": "~1.4.1",
    "@types/cors": "~2.8.5",
    "@types/dotenv": "~6.1.1",
    "@types/express": "~4.16.1",
    "@types/imagemin": "~7.0.0",
    "@types/jsonwebtoken": "~8.3.2",
    "@types/memory-cache": "~0.2.0",
    "@types/multer": "~1.3.7",
    "@types/multer-s3": "~2.7.6",
    "@types/node": "~12.0.12",
    "@types/nodemailer": "~6.2.1",
    "@types/sharp": "~0.17.10",
    "@types/uuid": "~3.4.4",
    "@types/validator": "~10.11.1",
    "@types/compression": "~1.0.1",
    "ts-node-dev": "~1.0.0-pre.39",
    "typescript": "~3.4.5"
  }
}
